{
  "manifest_version": 3,
  "name": "Save bookmark to root directory",
  "description": "__MSG_extDes__",
  "version": "1.0.0",
  "author": "pana",
  "homepage_url": "https://github.com/LightAPIs/save-bookmark-to-root-directory",
  "default_locale": "en",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon_16.png",
      "32": "icons/icon_32.png",
      "48": "icons/icon_48.png",
      "128": "icons/icon_128.png"
    },
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["bookmarks", "storage", "activeTab"],
  "key": ""
}
